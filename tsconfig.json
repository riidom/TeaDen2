{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "TeaDen",
    "compileOnSave": true,
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "preserveConstEnums": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "/js"
    },
    "include": ["./*"],
    "exclude": ["./js"]
}